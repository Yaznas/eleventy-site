{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "start": "eleventy --serve & postcss src/styles/*.css --dir _site/styles --watch",
    "build": "cross-env ELEVENTY_PRODUCTION=true eleventy && cross-env NODE_ENV=production postcss src/styles/*.css --dir _site/styles",
    "clean": "rimraf _site"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@types/tailwindcss": "^2.0.2",
    "cross-env": "^7.0.3",
    "luxon": "^1.25.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "eleventy-plugin-emoji": "^1.1.0",
    "eleventy-plugin-reading-time": "0.0.1",
    "netlify-cms-app": "^2.14.27",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "run-all": "^1.0.1",
    "tailwindcss": "^2.0.3"
  }
}
